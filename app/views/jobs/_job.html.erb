           <%= link_to job_path(job) do %>
           <!--<%= image_tag job.image.url(:original), class: "img-responsive" %> -->
          <% end %>
       
        </div>
        <div class="info">
            <span class="job_title"><%= link_to job.title, job_path(job) %></span>
